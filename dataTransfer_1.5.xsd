			<xsd:element name="text" type="xs:string"/>
			<xsd:element name="price" type="xs:double"/>
			<xsd:element name="reversal" type="xs:boolean" default="false" minOccurs="0"/>
			<xsd:element name="group" type="xs:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Option">
		<xsd:sequence>
			<xsd:element name="code" type="xs:string" minOccurs="0"/>
			<xsd:element name="text" type="xs:string"/>
			<xsd:element name="price" type="xs:double"/>
			<xsd:element name="print" type="xs:boolean" default="true" minOccurs="0"/>
			<xsd:element name="group" type="xs:string" minOccurs="0"/>
			<xsd:element name="purchasePrice" type="xs:double"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="Accessory">
		<xsd:sequence>
			<xsd:element name="code" type="xs:string" minOccurs="0"/>
			<xsd:element name="text" type="xs:string"/>
			<xsd:element name="price" type="xs:double"/>
			<xsd:element name="print" type="xs:boolean" default="true" minOccurs="0"/>
			<xsd:element name="group" type="xs:string" minOccurs="0"/>
			<xsd:element name="purchasePrice" type="xs:double"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="salesVehicleType">
		<xsd:sequence>
			<xsd:element name="vehicleNumber" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>with location global unique</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="location" type="xsd:int"/>
			<xsd:element name="header">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="stockState" type="StockState"/>
						<xsd:element name="salesState" type="SalesState"/>
						<xsd:element name="brand" type="xsd:string"/>
						<xsd:element name="modelShort" type="xsd:string" minOccurs="0"/>
						<xsd:element name="model" type="xsd:string" minOccurs="0"/>
						<xsd:element name="modelLong" type="xsd:string" minOccurs="0"/>
						<xsd:element name="ps" type="xsd:double" minOccurs="0"/>
						<xsd:element name="kw" type="xsd:double" minOccurs="0"/>
						<xsd:element name="color" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="number" minOccurs="0"/>
									<xsd:element name="name" type="xsd:string"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="firstRegistration" type="xsd:date" minOccurs="0"/>
						<xsd:element name="milage" type="xsd:int" minOccurs="0"/>
						<xsd:element name="standingTime" type="xsd:int" minOccurs="0"/>
						<xsd:element name="listPrice" type="xsd:double" minOccurs="0"/>
						<xsd:element name="actualSalesPrice" type="xsd:double" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>gross</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="vehicleType" type="vehicleTypeType"/>
						<xsd:element name="licensePlate" type="xsd:string" minOccurs="0"/>
						<xsd:element name="storingLocation" type="xsd:string" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="details" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="basis">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="salesman" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="id" type="xsd:string"/>
												<xsd:element name="name" type="xsd:string"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="buyer" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="location" type="xsd:int" minOccurs="0"/>
												<xsd:element name="id" type="xsd:int" minOccurs="0"/>
												<xsd:element name="name" type="xsd:string"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="vin" type="xsd:string" minOccurs="0"/>
									<xsd:element name="yearOfModel" type="xsd:gYear" minOccurs="0"/>
									<xsd:element name="interiorColor" type="xsd:string" minOccurs="0"/>
									<xsd:element name="doors" type="xsd:int" minOccurs="0"/>
									<xsd:element name="cubicCapacity" type="xsd:double" minOccurs="0"/>
									<xsd:element name="bodywork" type="xsd:string" minOccurs="0"/>
									<xsd:element name="fuelType" type="xsd:string" minOccurs="0"/>
									<xsd:element name="fuelDescription" type="xsd:string" minOccurs="0"/>
									<xsd:element name="nationalCode" type="xsd:int" minOccurs="0"/>
									<xsd:element name="serialNumber" type="xsd:string" minOccurs="0"/>
									<xsd:element name="engineNumber" type="xsd:string" minOccurs="0"/>
									<xsd:element name="keyNumber1" type="xsd:string" minOccurs="0"/>
									<xsd:element name="keyNumber2" type="xsd:string" minOccurs="0"/>
									<xsd:element name="radioCode" type="xsd:string" minOccurs="0"/>
									<xsd:element name="licensePlate" type="xsd:string" minOccurs="0"/>
									<xsd:element name="deRegistrationDate" type="xsd:date" minOccurs="0"/>
									<xsd:element name="numberPreOwners" type="xsd:int" minOccurs="0"/>
									<xsd:element name="namePrevOwner" type="xsd:string" minOccurs="0"/>
									<xsd:element name="taxNumberPrevOwner" type="xsd:string" minOccurs="0"/>
									
									
									<xsd:element name="kbaHsn" type="xsd:string" minOccurs="0"/>
									<xsd:element name="kbaTsn" type="xsd:string" minOccurs="0"/>
									<xsd:element name="manufacturingNumber" type="xsd:string" minOccurs="0"/>
									<xsd:element name="lastRegistrationDate" type="xsd:date" minOccurs="0"/>
									<xsd:element name="weight" type="xsd:int" minOccurs="0"/>
									<xsd:element name="payload" type="xsd:int" minOccurs="0"/>
									<xsd:element name="co2Gkm" type="xsd:double" minOccurs="0"/>
									<xsd:element name="noxMgkm" type="xsd:double" minOccurs="0"/>
									<xsd:element name="particulateEmission" type="xsd:double" minOccurs="0"/>
									<xsd:element name="fuelConsumptionCity" type="xsd:double" minOccurs="0"/>
									<xsd:element name="fuelConsumptionCountryside" type="xsd:double" minOccurs="0"/>
									<xsd:element name="fuelConsumptionCombined" type="xsd:double" minOccurs="0"/>
									<xsd:element name="ePASticker" type="EPASticker"/>
									<xsd:element name="testDate1" type="xsd:date" minOccurs="0"/>
									<xsd:element name="testDate2" type="xsd:date" minOccurs="0"/>
									<xsd:element name="euImport" type="xsd:boolean" minOccurs="0"/>
									<xsd:element name="wrack" type="xsd:boolean" minOccurs="0"/>
									<xsd:element name="supplierOrderNo" type="xsd:string" minOccurs="0"/>
									<xsd:element name="carLetter" type="xsd:string" minOccurs="0"/>
									<xsd:element name="carLetterLocation" type="xsd:string" minOccurs="0"/>
									<xsd:element name="deliveryDate" type="xsd:date" minOccurs="0"/>
									<xsd:element name="typeCode" type="xsd:string" minOccurs="0" />
									<xsd:element name="salesContractDate" type="xsd:date" minOccurs="0"/>
									
									<xsd:element name="nova" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>only for Austria</xsd:documentation>
										</xsd:annotation>
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="novaIncoming" type="xsd:boolean" minOccurs="0"/>
												<xsd:element name="novaRate" type="xsd:double" minOccurs="0"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="incomeOfVehicle" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="vat" type="xsd:boolean" minOccurs="0"/>
												<xsd:element name="salesman" minOccurs="0">
													<xsd:complexType>
														<xsd:sequence>
															<xsd:element name="id" type="xsd:string"/>
															<xsd:element name="name" type="xsd:string"/>
														</xsd:sequence>
													</xsd:complexType>
												</xsd:element>
												<xsd:element name="date" type="xsd:date" minOccurs="0"/>
												<xsd:element name="invoiceNumber" type="xsd:string" minOccurs="0"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="combinationNumber" type="xsd:string" minOccurs="0"/>
									<xsd:element name="extraState" type="xsd:string" minOccurs="0"/>
									<xsd:element name="remarks" type="xsd:string" minOccurs="0"/>
									<xsd:element name="others" type="xsd:string" minOccurs="0"/>
									<xsd:element name="specialEquipment1" type="xsd:string" minOccurs="0"/>
									<xsd:element name="specialEquipment2" type="xsd:string" minOccurs="0"/>
									<xsd:element name="specialEquipment3" type="xsd:string" minOccurs="0"/>
									<xsd:element name="vehicleType" type="vehicleTypeType"/>
									<xsd:element name="matchcode" type="xs:string" minOccurs="0"/>
									<xsd:element name="masterNumber" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>specially for Switzerland, so called "Stammnummer"</xsd:documentation>
										</xsd:annotation>
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="30"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="prices">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="options" type="xsd:double" minOccurs="0"/>
									<xsd:element name="accessories" type="xsd:double" minOccurs="0"/>
									<xsd:element name="equipment" type="xsd:double" minOccurs="0"/>
									<xsd:element name="others" type="xsd:double" minOccurs="0"/>
									<xsd:element name="novaVariance" type="xsd:double" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>only for Austria</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="marketPrice" type="xsd:double" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>gross</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="salesPrice1" type="xsd:double" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>gross</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="minimumSalesPrice" type="xsd:double" minOccurs="0">
										<xsd:annotation>
											<xsd:documentation>gross</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="purchase" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="vehicle" type="xsd:double" minOccurs="0"/>
												<xsd:element name="options" type="xsd:double" minOccurs="0"/>
												<xsd:element name="equipment" type="xsd:double" minOccurs="0"/>
												<xsd:element name="others" type="xsd:double" minOccurs="0"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="incomes" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="income" type="IncomeEffort" minOccurs="0" maxOccurs="unbounded"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
									<xsd:element name="efforts" minOccurs="0">
										<xsd:complexType>
											<xsd:sequence>
												<xsd:element name="effort" type="IncomeEffort" minOccurs="0" maxOccurs="unbounded"/>
												<xsd:element name="internEffort1" type="xsd:double" minOccurs="0"/>
												<xsd:element name="internEffort2" type="xsd:double" minOccurs="0"/>
												<xsd:element name="internEffort3" type="xsd:double" minOccurs="0"/>
												<xsd:element name="overheadCosts" type="xsd:double" minOccurs="0"/>
												<xsd:element name="commission" type="xsd:double" minOccurs="0"/>
											</xsd:sequence>
										</xsd:complexType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="options" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="option" type="Option" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="accessories" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="accessory" type="Accessory" minOccurs="0" maxOccurs="unbounded"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="externId" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="invoiceType">
		<xsd:sequence>
			<xsd:element name="invoiceID" type="xsd:int">
				<xsd:annotation>
					<xsd:documentation>unique invoice ID, for identifying</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="header">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="location" type="xsd:int"/>
						<xsd:element name="kindOfInvoice">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="vehicle"/>
									<xsd:enumeration value="workshop"/>
									<xsd:enumeration value="part"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="customer" type="invoiceCustomerType"/>
						<xsd:element name="differentInvoiceReceiver" type="invoiceCustomerType" minOccurs="0"/>
						<xsd:element name="vehicle" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="vehicleNumber" type="xsd:int">
										<xsd:annotation>
											<xsd:documentation>with location global unique</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="vin" type="xsd:string"/>
									<xsd:element name="vehicleType" type="vehicleTypeType"/>
									<xsd:element name="brand">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="2"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="type">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="20"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="licensePlate">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="10"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
									<xsd:element name="milage" type="xsd:int"/>
									<xsd:element name="motorNumber">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="20"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="invoiceDate" type="xsd:date"/>
						<xsd:element name="invoiceNumber" type="xsd:string"/>
						<xsd:element name="orderDate" type="xsd:date"/>
						<xsd:element name="orderNumber" type="xsd:string"/>
						<xsd:element name="acceptor" type="salesmanType"/>
						<xsd:element name="sumNet" type="xsd:double"/>
						<xsd:element name="vat" type="xsd:double"/>
						<xsd:element name="nova" type="xsd:double" minOccurs="0"/>
						<xsd:element name="sumGross" type="xsd:double"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="positions" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="position" minOccurs="1" maxOccurs="unbounded">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="positionNumber" type="xsd:int"/>
									<xsd:element name="number" type="xsd:string">
										<xsd:annotation>
											<xsd:documentation>number of part or number of working operation number of package...</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="title" type="xsd:string"/>
									<xsd:element name="amount" type="xsd:double"/>
									<xsd:element name="issuer" type="salesmanType"/>
									<xsd:element name="mechanic" type="salesmanType"/>
									<xsd:element name="sumBeforeDiscount" type="xsd:double"/>
									<xsd:element name="discount" type="xsd:double"/>
									<xsd:element name="sumAfterDiscount" type="xsd:double"/>
									<xsd:element name="vatRate" type="xsd:double"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="invoiceCustomerType">
		<xsd:sequence>
			<xsd:element name="customerNumber" type="customerIdentificationType"/>
			<xsd:element name="salutation">
				<xsd:annotation>
					<xsd:documentation>crosstable</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:totalDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name" minOccurs="1" maxOccurs="4">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="sequence" type="xsd:int">
							<xsd:annotation>
								<xsd:documentation>1 or 2 or 3 or 4</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="name">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="address">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="street">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="zip">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="city">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="country">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
		</xsd:sequence>
	</xsd:complexType>
</xsd:schema>
