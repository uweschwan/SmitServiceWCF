					<xsd:documentation>with customerLocation: unique identifier of customer in DMS</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="contactPersonNumber">
				<xsd:annotation>
					<xsd:documentation>with customerNumber: unique identifier of contactPerson in DMS</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:totalDigits value="6"/>
