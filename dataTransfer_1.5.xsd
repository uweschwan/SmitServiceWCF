			<xsd:element name="division" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>crosstable</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="street" type="xsd:string" minOccurs="0"/>
			<xsd:element name="country" type="xsd:string" minOccurs="0"/>
			<xsd:element name="zipCode" type="xsd:string" minOccurs="0"/>
			<xsd:element name="city" type="xsd:string" minOccurs="0"/>
			<xsd:element name="marketing" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="postWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="emailWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="faxWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="phoneWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="smsWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="unwantedMarketing" minOccurs="0" maxOccurs="2">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="sequence" type="xsd:int">
										<xsd:annotation>
											<xsd:documentation>1 or 2</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="type">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="4"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="preferredMarketing" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="post"/>
									<xsd:enumeration value="email"/>
									<xsd:enumeration value="fax"/>
									<xsd:enumeration value="phone"/>
									<xsd:enumeration value="sms"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="useData" type="xsd:boolean" nillable="true" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="newsletter" type="xsd:string" minOccurs="0"/>
			<xsd:element name="telPrivate" type="xsd:string" minOccurs="0"/>
			<xsd:element name="mobilePrivate" type="xsd:string" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="drivingLicenseType">
		<xsd:sequence>
			<xsd:element name="number" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="15"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="classes" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="authority" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="7"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="date" type="xsd:date" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="vehicleTypeType">
		<xsd:restriction base="xsd:string">
			<xsd:enumeration value="new"/>
			<xsd:enumeration value="demo"/>
			<xsd:enumeration value="used"/>
			<xsd:enumeration value="agency"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="FromToType">
		<xsd:sequence>
			<xsd:element name="from" type="xs:double" minOccurs="0"/>
			<xsd:element name="to" type="xs:double" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:simpleType name="StockState">
		<xsd:restriction base="xs:string">
			<xsd:enumeration value="bestellt"/>
			<xsd:enumeration value="lagernd"/>
			<xsd:enumeration value="ausgeliefert"/>
			<xsd:enumeration value="in Reparatur"/>
			<xsd:enumeration value="fiktiv"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="EPASticker">
		<xsd:restriction base="xs:string">
			<xsd:enumeration value="none"/>
			<xsd:enumeration value="red"/>
			<xsd:enumeration value="yellow"/>
			<xsd:enumeration value="green"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:simpleType name="SalesState">
		<xsd:restriction base="xs:string">
			<xsd:enumeration value="bestellt"/>
			<xsd:enumeration value="reserviert"/>
			<xsd:enumeration value="verfügbar"/>
		</xsd:restriction>
	</xsd:simpleType>
	<xsd:complexType name="FromToDateType">
		<xsd:sequence>
			<xsd:element name="from" type="xs:date" minOccurs="0"/>
			<xsd:element name="to" type="xs:date" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="IncomeEffort">
