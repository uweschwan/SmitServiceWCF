<?xml version="1.0" encoding="utf-8"?>
<!-- Mit XMLSpy v2011 rel. 2 (x64) (http://www.altova.com) von Kurt Fugger (fmade GmbH) bearbeitet -->
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema" xmlns:xs="http://www.w3.org/2001/XMLSchema">
	<xsd:element name="interfaceData">
		<xsd:complexType>
			<xsd:sequence>
				<xsd:element name="dataProvider" type="xsd:string">
					<xsd:annotation>
						<xsd:documentation>will be defined from Fmade (name of DMS)</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="interfaceVersion" type="xsd:int">
					<xsd:annotation>
						<xsd:documentation>version of the interface, if the interface will be extended</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="transmissionReason" nillable="true">
					<xsd:simpleType>
						<xsd:restriction base="xsd:string">
							<xsd:maxLength value="100"/>
						</xsd:restriction>
					</xsd:simpleType>
				</xsd:element>
				<xsd:element name="callingUser" type="salesmanType" nillable="true">
					<xsd:annotation>
						<xsd:documentation>for DMS authorization system if necessary</xsd:documentation>
					</xsd:annotation>
				</xsd:element>
				<xsd:element name="customer" type="customerType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="customerVehicle" type="customerVehicleType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="contactPerson" type="contactPersonType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="salesVehicle" type="salesVehicleType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="invoice" type="invoiceType" minOccurs="0" maxOccurs="unbounded"/>
				<xsd:element name="searchSalesVehicle" minOccurs="0">
					<xsd:complexType>
						<xsd:sequence>
							<xsd:element name="vehicleTypes" minOccurs="0">
								<xsd:complexType>
									<xsd:sequence>
										<xsd:element name="vehicleType" type="vehicleTypeType" minOccurs="0" maxOccurs="unbounded"/>
									</xsd:sequence>
								</xsd:complexType>
							</xsd:element>
							<xsd:element name="brand" type="xs:string" minOccurs="0" maxOccurs="unbounded">
								<xsd:annotation>
									<xsd:documentation>no occurance = all</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="model" type="xs:string" minOccurs="0"/>
							<xsd:element name="price" type="FromToType" minOccurs="0"/>
							<xsd:element name="ps" type="FromToType" minOccurs="0"/>
							<xsd:element name="fuelType" type="xs:string" minOccurs="0"/>
							<xsd:element name="standingTime" type="FromToType" minOccurs="0"/>
							<xsd:element name="milage" type="FromToType" minOccurs="0"/>
							<xsd:element name="bodywork" type="xs:string" minOccurs="0"/>
							<xsd:element name="location" type="xs:string" minOccurs="0"/>
							<xsd:element name="color" type="xs:string" minOccurs="0"/>
							<xsd:element name="stockState" type="StockState" minOccurs="0"/>
							<xsd:element name="salesState" type="SalesState" minOccurs="0"/>
							<xsd:element name="vehicleGroup" type="xs:string" minOccurs="0"/>
							<xsd:element name="firstRegistration" type="FromToDateType" minOccurs="0"/>
							<xsd:element name="specialEquipment" type="xs:string" minOccurs="0"/>
							<xsd:element name="salesman" type="xs:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>id of salesman who sold vehicle</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
							<xsd:element name="deliveryDate" type="FromToDateType" minOccurs="0"/>
							<xsd:element name="invoiceDate" type="FromToDateType" minOccurs="0"/>
							<xsd:element name="namePrevOwner" type="xs:string" minOccurs="0"/>
							<xsd:element name="nameBuyer" type="xs:string" minOccurs="0"/>
							<xsd:element name="orderDate" type="FromToDateType" minOccurs="0"/>
							<xsd:element name="vin" type="xs:string" minOccurs="0"/>
							<xsd:element name="vinShort" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>last 7 of vin</xsd:documentation>
								</xsd:annotation>
								<xsd:simpleType>
									<xsd:restriction base="xs:string">
										<xsd:maxLength value="7"/>
									</xsd:restriction>
								</xsd:simpleType>
							</xsd:element>
							<xsd:element name="licensePlate" type="xs:string" minOccurs="0"/>
							<xsd:element name="cominationNumber" type="xs:string" minOccurs="0"/>
							<xsd:element name="matchcode" type="xs:string" minOccurs="0"/>
							<xsd:element name="salesVehicleKey" type="xs:string" minOccurs="0">
								<xsd:annotation>
									<xsd:documentation>for searching a specific vehicle</xsd:documentation>
								</xsd:annotation>
							</xsd:element>
						</xsd:sequence>
					</xsd:complexType>
				</xsd:element>
			</xsd:sequence>
		</xsd:complexType>
	</xsd:element>
	<xsd:complexType name="customerType">
		<xsd:sequence>
			<xsd:element name="customerNumber" type="customerIdentificationType">
				<xsd:annotation>
					<xsd:documentation>with customerLocation: unique identifier of customer in DMS</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="deleted" type="xsd:boolean" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>customer is deleted</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="fsalesNumber" type="xsd:int" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>number in Fsales if customer was created in Fsales</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="externId" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>extern (DMS) unique Identifier of customer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="salutation" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>crosstable</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:totalDigits value="3"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="salutationLetter" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>salutation for letter,mail... f.e.: Dear Mr.</xsd:documentation>
				</xsd:annotation>
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="id" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>id of reference table</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:int">
									<xsd:totalDigits value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="description" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>extra text for salutation</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="salutationType" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="MALE"/>
						<xsd:enumeration value="FEMALE"/>
						<xsd:enumeration value="COMPANY"/>
						<xsd:enumeration value="FAMILY"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="firstName" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="lastName" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="25"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name" minOccurs="0" maxOccurs="4">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="sequence" type="xsd:int">
							<xsd:annotation>
								<xsd:documentation>1 or 2 or 3 or 4</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="name">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="matchcode" type="xsd:string" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>search string to find customer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="address" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="street" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="zip" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="10"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="city" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="25"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="country" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="3"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="state" type="xsd:int" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>address state, reference table, f.e.: moved</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="phone" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="private" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="business" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="mobil" minOccurs="0" maxOccurs="2">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="sequence" type="xsd:int">
										<xsd:annotation>
											<xsd:documentation>1 or 2</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="number">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="20"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="fax" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="email" minOccurs="0" maxOccurs="2">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="sequence" type="xsd:int">
							<xsd:annotation>
								<xsd:documentation>1 or 2</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="address">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="64"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="branche" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>reference table necessary</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="10"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="customerType" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:enumeration value="customer"/>
						<xsd:enumeration value="prospect"/>
						<xsd:enumeration value="lessor"/>
						<xsd:enumeration value="sub-dealer"/>
						<xsd:enumeration value="insurance"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="customerCategory" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>reference table</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="creation" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="date" type="xsd:date" minOccurs="0"/>
						<xsd:element name="location" type="xsd:int" minOccurs="0"/>
						<xsd:element name="salesman" type="salesmanType" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="lastChange" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="date" type="xsd:date" minOccurs="0"/>
						<xsd:element name="location" type="xsd:int" minOccurs="0"/>
						<xsd:element name="salesman" type="salesmanType" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="salesman" type="salesmanType" minOccurs="0"/>
			<xsd:element name="birthDate" type="xsd:date" minOccurs="0"/>
			<xsd:element name="extraCode" minOccurs="0" maxOccurs="3">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="sequence" type="xsd:int">
							<xsd:annotation>
								<xsd:documentation>1 or 2 or 3</xsd:documentation>
							</xsd:annotation>
						</xsd:element>
						<xsd:element name="code">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="1"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="balance" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="total" type="xsd:double" minOccurs="0"/>
						<xsd:element name="open" type="xsd:double" minOccurs="0"/>
						<xsd:element name="due" type="xsd:double" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="invoice" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="lastWokshop" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="date" type="xsd:date" minOccurs="0"/>
									<xsd:element name="location" type="xsd:int" minOccurs="0"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="lastPart" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="date" type="xsd:date" minOccurs="0"/>
									<xsd:element name="location" type="xsd:int" minOccurs="0"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="lastVehicle" minOccurs="0">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="date" type="xsd:date" minOccurs="0"/>
									<xsd:element name="location" type="xsd:int" minOccurs="0"/>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="lastContactDate" type="xsd:date" minOccurs="0">
				<xsd:annotation>
					<xsd:documentation>date of last contact with customer</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="marketing" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="postWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="emailWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="faxWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="phoneWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="smsWanted" type="xsd:boolean" nillable="true" minOccurs="0"/>
						<xsd:element name="unwantedMarketing" minOccurs="0" maxOccurs="2">
							<xsd:complexType>
								<xsd:sequence>
									<xsd:element name="sequence" type="xsd:int">
										<xsd:annotation>
											<xsd:documentation>1 or 2</xsd:documentation>
										</xsd:annotation>
									</xsd:element>
									<xsd:element name="type">
										<xsd:simpleType>
											<xsd:restriction base="xsd:string">
												<xsd:maxLength value="4"/>
											</xsd:restriction>
										</xsd:simpleType>
									</xsd:element>
								</xsd:sequence>
							</xsd:complexType>
						</xsd:element>
						<xsd:element name="preferredMarketing" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:enumeration value="post"/>
									<xsd:enumeration value="email"/>
									<xsd:enumeration value="fax"/>
									<xsd:enumeration value="phone"/>
									<xsd:enumeration value="sms"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="useData" type="xsd:boolean" nillable="true" minOccurs="0"/>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="numbers" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="vatNumber" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="companyRegistrationNumber" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="taxNumber" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="20"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="drivingLicense" type="drivingLicenseType" minOccurs="0"/>
			<xsd:element name="newsletter" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="1"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="ids" minOccurs="0">
				<xsd:complexType>
					<xsd:sequence>
						<xsd:element name="renaultID" type="xsd:integer" minOccurs="0"/>
						<xsd:element name="renaultR2ID" minOccurs="0">
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="50"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
						<xsd:element name="daciaID" type="xsd:integer" minOccurs="0"/>
						<xsd:element name="transferID" minOccurs="0">
							<xsd:annotation>
								<xsd:documentation>only Fsales is allowed to fill this id</xsd:documentation>
							</xsd:annotation>
							<xsd:simpleType>
								<xsd:restriction base="xsd:string">
									<xsd:maxLength value="30"/>
								</xsd:restriction>
							</xsd:simpleType>
						</xsd:element>
					</xsd:sequence>
				</xsd:complexType>
			</xsd:element>
			<xsd:element name="nextSaleDate" type="xsd:date" minOccurs="0"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="salesmanType">
		<xsd:sequence>
			<xsd:element name="identifier">
				<xsd:annotation>
					<xsd:documentation>unique identifier of salesman in DMS</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="4"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="name" type="xsd:string"/>
		</xsd:sequence>
	</xsd:complexType>
	<xsd:complexType name="customerVehicleType">
		<xsd:sequence>
			<xsd:element name="customerNumber" type="customerIdentificationType">
				<xsd:annotation>
					<xsd:documentation>with customerLocation: unique identifier of customer in DMS</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="vehicleNumber">
				<xsd:annotation>
					<xsd:documentation>with customerNumber: unique identifier of vehicle in DMS</xsd:documentation>
				</xsd:annotation>
				<xsd:simpleType>
					<xsd:restriction base="xsd:int">
						<xsd:totalDigits value="6"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="externId" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>extern (DMS) unique Identifier of vehicle</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="prevExternId" type="xsd:string" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>previouse extern (DMS) unique Identifier of vehicle if changed (if changeable)</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="vin">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="20"/>
					</xsd:restriction>
				</xsd:simpleType>
			</xsd:element>
			<xsd:element name="deleted" type="xsd:boolean" minOccurs="0" maxOccurs="1">
				<xsd:annotation>
					<xsd:documentation>vehicle is deleted</xsd:documentation>
				</xsd:annotation>
			</xsd:element>
			<xsd:element name="licensePlate" minOccurs="0">
				<xsd:simpleType>
					<xsd:restriction base="xsd:string">
						<xsd:maxLength value="10"/>
